<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="313.0" prefWidth="408.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.gmagro.PrimaryController">
   <children>
      <TextField fx:id="tf_log" alignment="CENTER" prefHeight="25.0" prefWidth="527.0" promptText="Login" text="alexis@gmail.com">
         <VBox.margin>
            <Insets top="50.0" />
         </VBox.margin>
      </TextField>
      <PasswordField fx:id="pf_pass" alignment="CENTER" promptText="password" text="toto">
         <VBox.margin>
            <Insets top="25.0" />
         </VBox.margin>
      </PasswordField>
      <Button fx:id="btn_connect" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#onClic" text="Connexion">
         <VBox.margin>
            <Insets top="25.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
